<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.bjfu.igarden.dao.DiseaseclassifyMapper">
    <insert id="addDiseaseClassifyByUserId" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO imgclassify (userid,image,place)
        VALUES (#{userid},#{image},#{place})
    </insert>
</mapper>